    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "nodemon": "^3.1.9",
    "prettier": "3.5.3",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.30.1"
  },
  "dependencies": {
    "dotenv": "^16.5.0"
